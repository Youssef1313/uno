<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net6.0-windows</TargetFramework>
		<TreatWarningsAsErrors>true</TreatWarningsAsErrors>
		<AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
		<UseWPF>true</UseWPF>
		<NoWarn>SYSLIB0014;CS1998</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<None Remove="Properties\launchSettings.json" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" Version="1.0.0" PrivateAssets="All" />
		<PackageReference Include="Microsoft.AppCenter" Version="$(MicrosoftAppCenterVersion)" />
		<PackageReference Include="Microsoft.VisualStudio.Services.InteractiveClient" Version="$(MicrosoftVisualStudioServicesInteractiveClientVersion)" />
		<PackageReference Include="Microsoft.TeamFoundationServer.Client" Version="$(MicrosoftTeamFoundationServerClientVersion)" />
		<PackageReference Include="NUnit" Version="3.13.3" />
		<PackageReference Include="NUnit.Engine" Version="$(NUnitEngineVersion)" />
		<PackageReference Include="Refit" Version="$(RefitVersion)" />
		<PackageReference Include="Mono.Options" Version="6.6.0.161" />
		<PackageReference Include="System.IO.Compression" Version="$(SystemIOCompressionVersion)" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Properties\" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="PresentationCore" />
		<Reference Include="PresentationFramework" />
		<Reference Include="System.Windows.Forms" />
		<Reference Include="WindowsBase" />
	</ItemGroup>

</Project>
